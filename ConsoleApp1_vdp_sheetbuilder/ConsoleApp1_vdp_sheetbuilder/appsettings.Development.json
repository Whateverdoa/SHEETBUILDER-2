{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UploadReliability": {
    "EnforceProgressForLarge": true,
    "LargeFileThresholdMb": 200,
    "IdempotencyActive": true,
    "RecentResultTtlMinutes": 30
  },
  "FileStorage": {
    "Directory": "uploads",
    "AutoDeleteAfterDownload": false,
    "MaxStorageAgeDays": 7
  }
}
